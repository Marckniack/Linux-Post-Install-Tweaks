[Desktop Entry]
Type=Service
X-KDE-ServiceTypes=KonqPopupMenu/Plugin
MimeType=application/*;
Actions=runWithdGPU
X-KDE-Submenu=NVidia Optimus

[Desktop Action runWithdGPU]
Name=Run on dedicated GPU
Icon=show-gpu-effects
Exec=__NV_PRIME_RENDER_OFFLOAD=1 __VK_LAYER_NV_optimus=NVIDIA_only __GLX_VENDOR_LIBRARY_NAME=nvidia %u
